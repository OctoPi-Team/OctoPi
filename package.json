{
	"name": "octo-pi",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@react-three/drei": "^9.66.1",
		"@react-three/fiber": "^8.13.0",
		"@testing-library/user-event": "^13.5.0",
